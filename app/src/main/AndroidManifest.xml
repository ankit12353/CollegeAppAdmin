<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.collegeadminapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeAdminApp">
        <activity android:name=".ui.notice.DeleteNoticeActivity"></activity>
        <activity android:name=".ui.faculty.AddUpdateFacultyActivity" />
        <activity android:name=".ui.faculty.UpdateFacultyActivity" />
        <activity android:name=".ui.UploadPdfActivity" />
        <activity android:name=".ui.UploadImageActivity" />
        <activity android:name=".ui.notice.UploadNoticeActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>